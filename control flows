import 'dart:io'; // Importing the dart:io library for I/O operations

void main() {
  // Prompt the user to enter a number
  stdout.write("Enter a number: "); // Displaying a prompt
  
  // Read the input as a string and parse it to an integer
  int number = int.parse(stdin.readLineSync()!); // Reading input from the user
  
  // Check the value of the number and print the corresponding message
  if (number > 10) {
    print("Your number is greater than 10"); // Printing message if number > 10
  } else if (number < 10) {
    print("Your number is less than 10"); // Printing message if number < 10
  } else {
    print("Your number is equal to 10"); // Printing message if number == 10
  }
}

